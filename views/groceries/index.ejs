<!DOCTYPE html>
<html>
  <head>
    <title>Grocery List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h2>Grocery List</h2>
    <table id="list">
      <thead>
        <tr>
          <th>Item</th>
          <th>Category</th>
          <th>Remove</th>
        </tr>
      </thead>
      <tbody>
        <% groceries.forEach(function(g) { %>
          <tr>
            <td><%= g.item %></td>
            <td><%= g.category %></td>
            <td></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
      <div class='row'>
    <h4><a href="/beverages">Beverages</a>
    <a href="/groceries/new">Dairy</a>
    <a href="/groceries/new">Meat</a>
    <a href="/groceries/new">Produce</a>
   <a href="/groceries/new">Snacks</a></h4>
    </div>
  </body>
</html>
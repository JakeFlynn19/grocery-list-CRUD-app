<!DOCTYPE html>
<html>
  <head>
    <title>Grocery List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h2>Grocery List</h2>
    <table id="list">
      <thead>
        <tr>
          <th>Item</th>
          <th>Category</th>
          <th>Remove</th>
        </tr>
      </thead>
      <tbody>
        <% groceries.forEach(function(g) { %>
          <tr>
            <td><%= g.item %></td>
            <td><%= g.category %></td>
            <td></td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    <h4><a href="/beverages">Beverages</a>
    <a href="/dairy">Dairy</a>
    <a href="/meat">Meat</a>
    <a href="/produce">Produce</a>
   <a href="/snacks">Snacks</a></h4>
   <h4><a href="/groceries/new">Add Groceries</a></h4>
  </body>
</html>